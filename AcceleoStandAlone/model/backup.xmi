<?xml version="1.0" encoding="ASCII"?>
<p:ViewModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://wwww.sofrecom.codegen"
    xsi:schemaLocation="http://wwww.sofrecom.codegen MetaModel.ecore">
  <entities
      name="Person">
    <attributes
        name="id"
        type="Long">
      <context
          key="isPrimaryKey"
          value="true"/>
      <context
          key="visible"
          value="true"/>
    </attributes>
    <attributes
        name="login"
        type="String">
      <context
          key="visible"
          value="true"/>
    </attributes>
    <attributes
        name="email"
        type="String">
      <context
          key="validation"
          value="email"/>
      <context
          key="visible"
          value="true"/>
    </attributes>
    <attributes
        name="lastName"
        type="String">
      <context
          key="visible"
          value="true"/>
    </attributes>
    <attributes
        name="firstName"
        type="String">
      <context
          key="visible"
          value="true"/>
    </attributes>
    <context
        key="visible"
        value="true"/>
    <context
        key="UIName"
        value="personnes"/>
  </entities>
</p:ViewModel>
